{
  "name": "webeact",
  "version": "0.2.5",
  "description": "Una librería para construir interfaces reactivas con web components, inspirada en los hooks de React/Preact y en las template de Astro. Optimizada para integración con servidores",
  "main": "src/index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "start": "node --run dev",
    "dev": "pnpm pack && pnpm -r dev",
    "prepack": "node --run lint",
    "lint": "eslint --fix ."
  },
  "keywords": [
    "web-components",
    "react-like",
    "astro-like",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyTechnologies/Webeact.git"
  },
  "bugs": {
    "url": "https://github.com/AndyTechnologies/Webeact/issues"
  },
  "homepage": "https://github.com/AndyTechnologies/Webeact#readme",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@eslint/css": "0.9.0",
    "@eslint/js": "9.29.0",
    "@eslint/json": "0.12.0",
    "eslint": "9.28.0",
    "globals": "16.2.0"
  },
  "engines": {
    "node": ">= 23.0.0"
  }
}
